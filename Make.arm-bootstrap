## Make.arm-bootstrap
#

arm-bootstrap: arm-toolchain
	$(MAKE) JTL_CROSS=arm JTL_CROSSCFLAGS="-mcpu=arm6 -msoft-float -O" \
		JTL_CROSSCPPFLAGS="-msoft-float -mcpu=arm6" \
		JTL_CROSSGCCFLAGS="--with-cpu=arm6 --enable-multilib" \
		JTL_CROSSGLIBCFLAGS=--without-fp arm-do-bootstrap
	@touch $(MADE)/arm-bootstrap
